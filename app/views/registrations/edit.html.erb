<h1>編輯個人資料</h1>

<%= form_for(current_user, url: users_path, method: 'put') do |f| %>

  <div class="fields">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="fields">
    <%= f.label :nickname, '暱稱' %>
    <%= f.text_field :nickname %>
  </div>

  <%= f.submit '更新' %>
<% end %>